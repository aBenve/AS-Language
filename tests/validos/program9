module miElemento as Canvas {
    Template: {
        top:[otroElemento] 
    }
    Style:css{
        color:red;    
    }css
}


module otroElemento{
    Template: {
        bottom:"hola"
    }
    Script: js{
        
        let list = []
        let n = 100
        for i in n {
            list.add(`Product nÂ°${i}`)
        }
        list.mapToElement("top" ,s -> Element(content: s, background: Color("green")))
    }js
}