module Canvas {
    Template: {
        rightCenter:[a],
        center:[b],
        leftCenter:[c]
    }
    Style: css{
        #a{
            color: red;
        }
        #b{
            color: blue;
        }
        #c{
            color: green;
        }
    }css
}

module a {
    Template:{
        children:"soy a"
    }
    
}

module b{
    Template:{
        children:"soy b"
    }
    Style: css{}css
}

module c {
    Template:{
        children:"soy c"
    }
}