~ Esto es un comentario ~

module miElemento as Canvas {
    Template: {
        top:${miVariable}
    }
    Script:js{
        let count = 0;
        let __miVariable
        if ( count < 0 ) {
            __miVariable = "negativo"
        }
        else{
            __miVariable = "positivo"
        }
    }js
}